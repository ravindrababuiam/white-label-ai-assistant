{
  "name": "agency-mcp-integration",
  "version": "1.0.0",
  "description": "Agency MCP server integration for white-label AI assistant platform",
  "main": "dist/agency-integration-manager.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/agency-integration-manager.js",
    "dev": "tsx watch src/agency-integration-manager.ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:integration": "vitest --config vitest.integration.config.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "validate-config": "node dist/validate-config.js"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "agency",
    "integration",
    "ai-assistant"
  ],
  "author": "White-label AI Assistant Team",
  "license": "MIT",
  "dependencies": {
    "open-webui-mcp-integration": "file:../open-webui-mcp-integration"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/",
    "config/",
    "README.md"
  ]
