# Database Configuration
POSTGRES_PASSWORD=your_secure_postgres_password

# LiteLLM Configuration
LITELLM_API_URL=http://localhost:4000
LITELLM_MASTER_KEY=your_litellm_master_key

# Lago Configuration
LAGO_API_URL=http://localhost:3000
LAGO_API_KEY=your_lago_api_key
LAGO_WEBHOOK_SECRET=your_lago_webhook_secret

# Service Configuration
LOG_LEVEL=INFO
WEBHOOK_TIMEOUT=30
MAX_RETRIES=3
RETRY_DELAY=60
BATCH_SIZE=100

# Monitoring Configuration
ENABLE_METRICS=true

# Customer Mapping Configuration
DEFAULT_ORGANIZATION_ID=your_default_lago_organization_id

# Rate Limiting Configuration
RATE_LIMIT_REQUESTS_PER_MINUTE=1000
RATE_LIMIT_BURST=100

# Error Handling Configuration
DEAD_LETTER_QUEUE_ENABLED=true
ERROR_NOTIFICATION_WEBHOOK=https://your-monitoring-system.com/webhooks/errors