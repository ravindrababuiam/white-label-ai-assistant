# Database Configuration
POSTGRES_PASSWORD=your_secure_postgres_password

# Encryption Keys (Generate secure random keys)
SECRET_KEY_BASE=your_secret_key_base_64_chars_minimum
ENCRYPTION_PRIMARY_KEY=your_encryption_primary_key_32_chars
ENCRYPTION_DETERMINISTIC_KEY=your_encryption_deterministic_key_32_chars
ENCRYPTION_KEY_DERIVATION_SALT=your_encryption_key_derivation_salt_32_chars

# Application URLs
LAGO_API_URL=http://localhost:3000
LAGO_FRONT_URL=http://localhost:8080

# Email Configuration (Optional)
LAGO_SMTP_ADDRESS=smtp.gmail.com
LAGO_SMTP_PORT=587
LAGO_SMTP_USERNAME=your_email@gmail.com
LAGO_SMTP_PASSWORD=your_app_password

# Webhook Configuration
LAGO_WEBHOOK_SECRET=your_webhook_secret_key

# License Configuration (for self-hosted)
LAGO_LICENSE=your_lago_license_key

# Sentry Configuration (Optional)
SENTRY_DSN=your_sentry_dsn_optional

# AWS Configuration (Optional for S3 storage)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1
AWS_S3_BUCKET=your-lago-storage-bucket

# Frontend Configuration
LAGO_DISABLE_SIGNUP=true

# Integration Configuration
LITELLM_WEBHOOK_URL=http://litellm-webhook-service:3001/webhooks/usage